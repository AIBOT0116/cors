{
    "version": 2,
    "public": false,
    "rewrites": [
      {
        "source": "/proxy/(.*)",
        "destination": "/api/proxy"
      },
      {
        "source": "/proxy",
        "destination": "/api/proxy"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, PUT, DELETE, OPTIONS"
          }
        ]
      }
    ]
  }